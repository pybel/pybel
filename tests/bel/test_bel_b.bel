# Statements Section

SET STATEMENT_GROUP = "Group with new and overlapping edges to test_bel"

# The following statements have overlap with test_bel

SET Citation = {"PubMed","That one article from last week","123455","2012-01-31","Example Author|Example Author2"}
SET Species = "9606"

SET Evidence = "Evidence 1 \
w extra notes"

p(HGNC:AKT1) -> p(HGNC:EGFR)

SET Evidence = "Evidence 2"
SET CellLine = "10B9 cell"

p(HGNC:EGFR) -| p(HGNC:FADD)

# The following statements do not have overlap and should be deleted by cascade
r(HGNC:AKT1) >> p(HGNC:AKT1)

SET Citation = {"PubMed", "The new article that survives cascade wipe", "4711345", "2016-07-12", "New Author|Example Author2"}
SET Evidence = "An evidence that will be deleted by cascade"

p(HGNC:AKT1) =| act(p(HGNC:GSK3B), ma(kin))


