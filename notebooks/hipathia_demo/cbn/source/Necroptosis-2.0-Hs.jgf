            {
              "bel_statement": "p(HGNC:MAP3K7) actsIn kin(p(HGNC:MAP3K7))",
              "citation": {
                "type": "PubMed",
                "id": "15229469",
                "name": "Nat Rev Immunol 2004 Jul 4(7) 499-511"
