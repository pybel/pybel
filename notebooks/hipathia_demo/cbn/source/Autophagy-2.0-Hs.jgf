              }
            },
            {
              "bel_statement": "p(HGNC:PDPK1) actsIn kin(p(HGNC:PDPK1))",
              "citation": {
                "type": "PubMed",
                "id": "15890450",
                "name": "Biochim Biophys Acta 2005 Oct 10 1725(3) 340-7"
              },
              "experiment_context": {
