            {
              "bel_statement": "p(HGNC:TBX21) actsIn tscript(p(HGNC:TBX21))",
              "citation": {
                "type": "PubMed",
                "id": "11994418",
                "name": "J Exp Med 2002 May 6 195(9) 1129-43"
              },
              "experiment_context": {
