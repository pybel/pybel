            },
            {
              "bel_statement": "p(HGNC:TBX21) actsIn tscript(p(HGNC:TBX21))",
              "citation": {
                "type": "PubMed",
                "id": "17195845",
                "name": "Nat Immunol 2007 Feb 8(2) 145-53"
              },
              "experiment_context": {
                "disease": "",
                "species_common_name": "Mouse",
                "tissue": "",
                "cell": "Th1 cell"
              },
              "summary_text": "both transcription factors T-bet and Runx3 are required for maximal production of interferon-gamma (IFN-gamma) and silencing of the gene encoding interleukin 4 (Il4) in T(H)1 cells.",
              "metadata": {
                "id": "53e41892ec93341d30df0b2c",
