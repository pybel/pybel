          "casual": false,
          "evidences": [
            {
              "bel_statement": "tscript(p(HGNC:E2F1)) increases p(HGNC:CDKN2C)",
              "citation": {
                "type": "Other",
                "id": "21543644",
                "name": "J Immunol. 2011 Jun 15;186(12):6673-82. doi: 10.4049/jimmunol.1002544. Epub 2011 May 4."
              },
              "experiment_context": {
                "disease": "",
                "species_common_name": "mouse",
                "tissue": "",
                "cell": ""
              },
              "summary_text": "Cdkn2c expression is regulated by the E2F1 transcription factor, and the -1209 and -279 constructs contain two sites that loosely fit the consensus DNA sequence of E2F-binding sites. As a preliminary to a detailed analysis of the Cdkn2c promoter, we extended these experiments to determine whether the difference of expression between the Sle2c1 and B6 allele is maintained in the presence of E2F1. E2F1 increased the activity of the Cdkn2c promoter constructs −1209 and −279 by 190 and 225 folds, respectively.",
              "metadata": {
                "id": "552970e589e3620d90d234d0",
                "created_by": "alejandroferreiromorales.cr"
              }
            }
          ]
        }
