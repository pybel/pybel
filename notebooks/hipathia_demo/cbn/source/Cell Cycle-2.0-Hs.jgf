      {
        "source": "p(HGNC:GSK3B)",
        "relation": "actsIn",
        "target": "kin(p(HGNC:GSK3B))",
        "directed": false,
        "label": "p(HGNC:GSK3B) actsIn kin(p(HGNC:GSK3B))",
