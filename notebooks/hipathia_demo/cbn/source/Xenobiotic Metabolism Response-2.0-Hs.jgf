      {
        "source": "p(HGNC:NFE2L2)",
        "target": "tscript(p(HGNC:NFE2L2))",
        "relation": "actsIn",
        "directed": true,
        "label": "p(HGNC:NFE2L2) actsIn tscript(p(HGNC:NFE2L2))",
