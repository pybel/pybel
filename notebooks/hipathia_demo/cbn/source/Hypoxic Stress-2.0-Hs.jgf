            {
              "bel_statement": "complex(p(HGNC:TSC1),p(HGNC:TSC2)) hasComponent p(HGNC:TSC1)",
              "summary_text": "in vitro; in vivo; Two-hybrid",
              "citation": {
                "type": "PubMed",
                "name": "Hum Mol Genet 2000 Jul 22 9(12) 1721-7",
                "id": "10915759"
              },
              "metadata": {
                "created_by": "selventa",
                "id": "557706562f9e2406902ee234"
